{
  "manifest_version": 2,
  "name": "twitch shortcuts",
  "description": "twitch tv shortcuts",
  "version": "1.0",
  "icons": {
    "128": "twitch-128.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "web_accessible_resources": ["index.js"],
  "permissions": [
    "tabs",
    "activeTab",
    "notifications"
  ]
}